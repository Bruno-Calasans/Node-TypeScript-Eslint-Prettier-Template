{
	"compilerOptions": {
	
		"target": "es2016",
		"module": "CommonJS",
		"rootDir": "./src",
		"moduleResolution": "Node",
		"outDir": "./build",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"sourceMap": true
	},
	"include": ["src/**/*.ts"],
	"exclude": ["node_modules"],
}
